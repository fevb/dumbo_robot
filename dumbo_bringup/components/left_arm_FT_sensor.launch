<?xml version="1.0"?>
<launch>

  <node name="left_arm_FT_sensor" pkg="dumbo_FT_sensor" type="dumbo_force_torque" cwd="node" respawn="false" output="screen">
    <rosparam command="load" file="$(find dumbo_hardware_config)/dumbo/config/left_arm_FT_sensor.yaml" />
    <remap from="/left_arm_FT_sensor/imu" to="/imu/data" />
  </node>

  <!-- FT sensor location -->
  <!-- <node ns="left_arm_FT_sensor" pkg="tf" type="static_transform_publisher" name="tf_publisher_FT" -->
  <!--       args="0 0 0.218 0 0 0  /left_arm_7_link /left_arm_FT_sensor 100" />   -->

  <!-- End effector center of mass location -->
  <node ns="left_arm_FT_sensor" pkg="tf" type="static_transform_publisher" name="tf_publisher_EE_CenterMass"
        args="0.00340815 0.0 0.07982 0 0 0  /left_arm_FT_sensor /left_arm_EndEffector_CenterMass 100" />  

</launch>
